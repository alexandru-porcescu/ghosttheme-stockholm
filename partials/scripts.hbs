<script>
$(document).ready(function() {
  // Order matters!!
  $('.post.tag-retina img').attr('data-rjs', 2);
  $('.post.tag-retina-hd img').attr('data-rjs', 3);
  $('.post.tag-retina-4k img').attr('data-rjs', 4);

  $('img').each(function(obj, value){
    var image = $(this).attr('href').text();
    if (image.indexOf("cloudinary.com") == 0){
      var cloud = 'https://res.cloudinary.com/hackers-and-slackers/image/upload/images/';
      var filename = image.split('/')[-1];
      var newURL = cloud + filename;
      $(this).attr('href, newURL');
      console.log(newURL);
    }
  });
});
</script>
